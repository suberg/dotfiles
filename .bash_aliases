# common
alias lsa='ls -a'
alias reload='source .bashrc'

# Find and delete empty directories
alias emptyDir='find . -empty -type d -delete'

# yarn
alias y='yarn'
alias ys='yarn start'

# git
alias g='git'
alias gra='git remote add'
alias gs='git status'
alias gi='git init'

# cd aliases
alias home='cd ~'
alias ..='cd ..'
alias ...='cd ../../../'
alias ....='cd ../../../../'
alias .....='cd ../../../../'
